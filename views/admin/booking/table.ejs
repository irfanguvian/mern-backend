<!-- DataTales Example -->
<div class="card shadow mb-4">
  <div class="card-header py-3">
    <h6 class="m-0 font-weight-bold text-primary">DataTables Example</h6>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table
        class="table table-bordered"
        id="dataTable"
        width="100%"
        cellspacing="0"
      >
        <thead>
          <tr>
            <th>No</th>
            <th>invoice</th>
            <th>Date</th>
            <th>Title</th>
            <th>Name Member</th>
            <th>Status</th>
            <th>Action</th>
          </tr>
        </thead>
        <tbody>
          <%booking.map((item,index)=>{ %>
          <tr>
            <td><%= index +1 %></td>
            <td><%= item.invoice %></td>
            <td>
              <%= item.bookingStartDate.toLocaleString().split(',')[0] %> s/d
              <%= item.bookingEndDate.toLocaleString().split(',')[0] %>
            </td>
            <td><%= item.itemId.title %></td>
            <td><%= item.memberId.firstName %></td>
            <td><%= item.payments.status %></td>
            <td>
              <a
                href="/admin/booking/<%= item._id %> "
                class="btn btn-success btn-circle btn-sm"
                ><i class="fas fa-eye"></i
              ></a>
            </td>
          </tr>
          <% }) %>
        </tbody>
      </table>
    </div>
  </div>
</div>
