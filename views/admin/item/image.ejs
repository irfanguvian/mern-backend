<div
  class="tab-pane fade show active"
  id="show-image"
  role="tabpanel"
  aria-labelledby="home-tab"
>
  <div class="row">
    <!-- DataTales Example -->
    <% images.imageId.map((image,index)=>{ %>
    <div class="col-md-4">
      <div class="card shadow mb-4">
        <div class="card-body">
          <img
            src="http://localhost:8000/<%= image.imageUrl %> "
            alt="image-<%= index %> "
            width="100%"
            display="block"
          />
        </div>
      </div>
    </div>
    <% }) %>
  </div>
  <a href="/admin/item" class="btn btn-warning">Back</a>
</div>
